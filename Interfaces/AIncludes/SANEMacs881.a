  				bpl.s   	@1
  				bclr		#7,d0
  				neg.l   	d0
@1				addq		#8,sp
  				ENDM

  				MACRO
  				FREMC
  				fmove.x 	([sp]),fp1
  				jsr 		_FC2X
  				frem		fp0,fp1
  				fmove   	fp1,fp0
  				fmove.x 	fp0,([sp])
  				fmove   	fpsr,d0
  				bfextu  	d0{8:8},d0
  				bpl.s   	@1
  				bclr		#7,d0
  				neg.l   	d0
@1				addq		#8,sp
  				ENDM

  				MACRO
  				FREMI
  				fmove.x 	([sp]),fp0
  				frem.w  	([4,sp]),fp0
  				fmove.x 	fp0,([sp])

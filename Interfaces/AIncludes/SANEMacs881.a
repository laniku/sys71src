;ÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑ
; Logb.
;ÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑ
  				MACRO
  				FLOGBX
  				fmove.x 	([sp]),fp0
  				fbne.w  	@1
  				flog2   	fp0
  				bra.s   	@3
@1				fmove   	fpsr,-(sp)
  				andi.l  	#$02000000,(sp)+
  				beq.s   	@2
  				fabs		fp0
  				bra.s   	@3
@2				fgetexp 	fp0,fp0
@3				fmove.x 	fp0,([sp])
  				addq		#4,sp
  				ENDM

;ÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑ
; Scalb.
;ÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑ
  				MACRO
  				FSCALBX
  				jsr 		_fprocENTRYsp
  				move.w  	([sp]),([sp],2)
  				addq.l  	#2,(sp)
  				MOVE.W  	#FFINT+FOSCALB,-(SP)
  				_FP68K
  				fmove.x 	fp0,([12,sp])
  				jsr 		_fprocEXITsp
  				ENDM

;ÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑ
; Copy-sign.
;ÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑ
  				MACRO
  				FCPYSGNX
  				bclr		#7,([4,sp])
  				tst.b   	([sp])
  				bpl.s   	@1
  				bset		#7,([sp])
@1				addq.l  	#8,sp
  				ENDM

;ÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑÑ

useDefault		DS.B	1 ;		Boolean
reserved2		DS.B	1 ;		Byte
userName		DS.L	1 ;		StringPtr
startPb			DS.L	1 ;		PPCStartPBPtr
				ENDR

GetDefaultUserParams	RECORD	0 ;
